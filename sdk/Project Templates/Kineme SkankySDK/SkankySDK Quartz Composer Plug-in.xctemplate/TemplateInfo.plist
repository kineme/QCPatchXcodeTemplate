			</array>
			<key>ProductType</key>
			<string>com.apple.product-type.bundle</string>
			<key>SharedSettings</key>
			<dict>
				<key>INSTALL_PATH</key>
				<string>$(HOME)/Library/Graphics/Quartz Composer Patches</string>
				<key>WRAPPER_EXTENSION</key>
				<string>plugin</string>
				<key>COMBINE_HIDPI_IMAGES</key>
				<string>YES</string>
				<key>FRAMEWORK_SEARCH_PATHS</key>
				<string>$(inherited) &quot;$(SYSTEM_APPS_DIR)/Xcode.app/Contents/Developer/Library/Frameworks&quot; /Library/Frameworks</string>
			</dict>
		</dict>
	</array>
