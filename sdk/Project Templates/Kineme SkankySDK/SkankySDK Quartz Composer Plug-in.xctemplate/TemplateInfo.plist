&lt;key&gt;CFBundleShortVersionString&lt;/key&gt;
&lt;string&gt;1.1&lt;/string&gt;
&lt;key&gt;NSPrincipalClass&lt;/key&gt;
&lt;string&gt;___PROJECTNAMEASIDENTIFIER___Principal&lt;/string&gt;
&lt;key&gt;GFPlugIn&lt;/key&gt;
&lt;true/&gt;
</string>
		<key>___PROJECTNAMEASIDENTIFIER___-Prefix.pch</key>
